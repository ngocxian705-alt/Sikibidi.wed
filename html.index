<!DOCTYPE html>
<html lang="vi">
<head>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>Sikibidi API MENU</title>
<style>
    body {
        font-family: Arial, sans-serif;
        margin: 0;
        background: #111;
        color: #fff;
    }
    header {
        background: #202020;
        padding: 15px;
        text-align: center;
        font-size: 22px;
        font-weight: bold;
    }
    .container {
        padding: 20px;
    }
    input, select, button {
        width: 100%;
        padding: 10px;
        margin: 10px 0;
        border-radius: 6px;
        border: none;
    }
    button {
        background: #ff0077;
        color: white;
        font-size: 16px;
    }
    .output {
        background: #222;
        padding: 10px;
        border-radius: 6px;
        margin-top: 15px;
        white-space: pre-wrap;
    }
    /* MENU G·ª¢I √ù EMOTE */
    .side-menu {
        position: fixed;
        top: 0;
        left: 0;
        width: 180px;
        height: 100%;
        overflow-y: auto;
        background: #181818;
        padding: 10px;
        border-right: 2px solid #333;
    }
    .side-menu h3 {
        font-size: 18px;
        margin-bottom: 10px;
        text-align: center;
    }
    .side-menu ul {
        list-style: none;
        padding: 0;
    }
    .side-menu li {
        margin: 6px 0;
        padding: 5px;
        background: #333;
        border-radius: 5px;
        font-size: 14px;
        text-align: center;
    }
    .content {
        margin-left: 200px;
        padding: 20px;
    }
</style>
</head>
<body>
<header>‚≠ê WELCOME SIKIBIDI MENU ‚≠ê</header>

<div class="side-menu">
    <h3>Emote G·ª£i √ù</h3>
    <ul id="emoteList"></ul>
</div>

<div class="content">
    <div class="container">
        <h2>API EMOTE (Team code + UID + Emote)</h2>
        <input id="team" placeholder="Nh·∫≠p team code" />
        <input id="uid1" placeholder="UID 1" />
        <input id="uid2" placeholder="UID 2" />
        <input id="uid3" placeholder="UID 3" />
        <input id="uid4" placeholder="UID 4" />
        <input id="emote" placeholder="T√™n emote (vd: m60, longtoc, xe...)" />
        <button onclick="runEmote()">üî• RUN EMOTE</button>
        <div id="out1" class="output"></div>

        <h2>API Ghost Command (Team code)</h2>
        <input id="teamGhost" placeholder="Nh·∫≠p team code" />
        <button onclick="runGhost()">üëª RUN GHOST</button>
        <div id="out2" class="output"></div>

        <h2>API TEAM 5 (UID + Region VN)</h2>
        <input id="uid5" placeholder="Nh·∫≠p UID" />
        <button onclick="runTeam5()">‚≠ê RUN TEAM 5</button>
        <div id="out3" class="output"></div>

        <h2>API Lag (Team code)</h2>
        <input id="teamLag" placeholder="Nh·∫≠p team code" />
        <button onclick="runLag()">‚ö° RUN LAG</button>
        <div id="out4" class="output"></div>
    </div>
</div>

<script>
// MAP EMOTE
const emoteMap = {
    "m60": "909051003","soka": "909000068","mangxa": "909000075","longtoc": "909000081","phomat": "909000090","m4a1": "909033001","camco": "909000034","xm8": "909000085","ump": "909000098","mp5": "909033002","m1887": "909035007","m1014lv8": "909039011","thomson": "909038010","g18": "909038012","an94": "909035012","goda": "909041005","mp40tiachop": "909040010","lv100": "909042007","chimgokien": "909042008","noel": "909051002","cungten": "909051012","xe": "909051010","canhhong": "909051001","cuoi": "909051015","voisen": "909051004","choigame": "909051017",
    "cuoi2": "909000002","chongday": "909000012","chicken": "909000006","ngaivang": "909000014","aicap": "909000011","tanghoa": "909000010","buocdicuaquy": "909000020","dakungfu": "909000028","thatim": "909000045","AK": "909000063","nhayvoicho": "909000052","saitama1": "909000064","siu": "909000066","mangxa4nguoi": "909000071","xemangxa": "909000074","cudam": "909000067","namdam": "909037011","chayraimoney": "909035001","Ctsinhton": "909034011","tamnang": "909036006","xevang4banh": "909040001","baytrenkiem": "909041004","xichdu": "909040013","sikibidi": "909042017","xemoto": "909043009","xe_lambo": "909042012","traitim2nguoi": "909043010","cauhon": "909043013","zombie": "909044012","parafal": "909045001","maico": "909045009","ngoithien": "909045015","cuoingua": "909045003","phao": "909045005","bapbenh": "909045012","6nong": "909046010","camco2": "909046013","sutbong": "909046015","nhaybong": "909046016","lacdich": "909046017","ngoighe": "909047001","ghetinhyeu": "909047003","canh1": "909047004","ngaivang2": "909047005","khoecup": "909047006","rasengan": "909047015","bithuatlangla": "909047016","ketannaruto": "909047017","chayninja": "909047018","ketannaruto2": "909047019","guitar": "909048003","piano": "909048004","nhaydanhtrong": "909048005","chacchac": "909048006","ngu": "909048007","lacda": "909048009","camco3": "909048011","nhaydengiau": "909048012","nailoong": "909049001","songaychoi": "909049006","p90": "909049010","oi": "909049013","uethochuyensinh": "909050002","hoadon": "909050005","minato": "909050006","The_Rings": "909050009","sungcuoi": "909050020"
};

// In g·ª£i √Ω v√†o menu tr√°i
let UL = document.getElementById("emoteList");
Object.keys(emoteMap).forEach(name => {
    let li = document.createElement("li");
    li.textContent = name;
    UL.appendChild(li);
});

async function runEmote() {
    let team = document.getElementById("team").value;
    let u1 = document.getElementById("uid1").value;
    let u2 = document.getElementById("uid2").value;
    let u3 = document.getElementById("uid3").value;
    let u4 = document.getElementById("uid4").value;
    let emo = document.getElementById("emote").value.trim().toLowerCase();

    if (!emoteMap[emo]) {
        document.getElementById("out1").textContent = "‚ùå Emote kh√¥ng t·ªìn t·∫°i!";
        return;
    }

    let id = emoteMap[emo];
    let url = `https://ff-community-apiemoteessss.onrender.com/emote?teamcode=${team}&uid1=${u1}&uid2=${u2}&uid3=${u3}&uid4=${u4}&emote_id=${id}`;

    let res = await fetch(url);
    let data = await res.text();
    document.getElementById("out1").textContent = data;
}

async function runGhost() {
    let team = document.getElementById("teamGhost").value;
    let url = `https://ghost-code-amph.onrender.com/execute_command_all?command=/bngx=${team}`;
    let res = await fetch(url);
    let data = await res.text();
    document.getElementById("out2").textContent = data;
}

async function runTeam5() {
    let uid = document.getElementById("uid5").value;
    let url = `https://ff-community-apiemoteessss.onrender.com/5?uid=${uid}&region=VN`;
    let res = await fetch(url);
    let data = await res.text();
    document.getElementById("out3").textContent = data;
}

async function runLag() {
    let team = document.getElementById("teamLag").value;
    let url = `https://ff-community-apiemoteessss.onrender.com/lag?teamcode=${team}`;
    let res = await fetch(url);
    let data = await res.text();
    document.getElementById("out4").textContent = data;
}
</script>
</body>
</html>
